    {
        "id": 5,
        "event_type": "RAW",
        "pattern": "^:(\\S+)!(\\S+)@(\\S+) QUIT(?: :(.+))?$",
        "action_type": "PYTHON",
        "action_content": "\nif event_data['command'] == 'QUIT':\n    nick = event_data['prefix'].split('!')[0]\n    reason = event_data['trailing'] if event_data['trailing'] else \"\"\n    for ctx_name, ctx in client.context_manager.contexts.items():\n        if ctx.type == \"channel\" and nick in ctx.users:\n            client.add_message(f\"{nick} quit{' (' + reason + ')' if reason else ''}\", \"join_part\", context_name=ctx_name)\n",
        "is_enabled": true,
        "is_regex": true,
